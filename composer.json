{
    "name": "citizen63000/easy-api-tests",
    "description": "",
    "keywords": ["api", "rest", "tests"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "citizen63000"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/citizen63000/easy-api-bundle.git"
        }
    ],
    "require": {
        "php": ">=7.4.3",
        "citizen63000/easy-api-bundle": "dev-master",
        "friendsofphp/php-cs-fixer": "@stable",
        "phpunit/phpunit": "6.*",
        "symfony/phpunit-bridge": "^3.0"
    },
    "config": {
        "discard-changes": true,
        "platform": {
            "php": "7.4.3"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "EasyApiTests\\": ""
        }
    }
}
