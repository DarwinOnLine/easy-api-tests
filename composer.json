{
    "name": "citizen63000/easy-api-tests",
    "description": "",
    "keywords": ["api", "rest", "tests"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "citizen63000"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/citizen63000/easy-api-bundle.git"
        }
    ],
    "require": {
        "php": ">=7.4.3",
        "citizen63000/easy-api-bundle": "dev-master",
        "guzzlehttp/psr7": "^1.9",
        "php-http/guzzle6-adapter": "^1.1",
        "phpunit/phpunit": "9.*",
        "symfony/phpunit-bridge": "^3.4",
        "phpunit/php-code-coverage": "9.*",
        "symfony/browser-kit": "^4.4",
        "symfony/web-profiler-bundle": "^4.4",
        "symfony/debug": "^4.4",
        "symfony/stopwatch": "^4.4"
    },
    "config": {
        "discard-changes": true,
        "platform": {
            "php": "7.4.3"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "EasyApiTests\\": "src/Framework"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
